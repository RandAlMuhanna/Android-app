<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    android:visibility="visible"
    tools:context=".home">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/map"
        android:layout_marginEnd="96dp"
        android:layout_weight="1"
        android:text="Map"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints,RtlCompat"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="423dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="127dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/sc2"
        android:layout_below="@+id/sc2"
        android:text="specializations and communication"
        android:textColor="@color/colorPrimaryDark"
        android:textStyle="bold"
        tools:ignore="MissingConstraints,RtlCompat"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="423dp" />

    <ImageButton
        android:id="@+id/map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/ic_location_on_black_24dp"
        tools:ignore="MissingConstraints,RtlCompat,VectorDrawableCompat"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="328dp" />

    <ImageButton
        android:id="@+id/sc2"
        android:layout_width="wrap_content"
        android:layout_height="89dp"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="61dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/ic_people_black_24dp"
        tools:ignore="MissingConstraints,RtlCompat,VectorDrawableCompat"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="340dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/aboutus"
        android:layout_marginStart="74dp"
        android:layout_weight="1"
        android:text="About us"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints,RtlCompat"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="273dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="66dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView3"
        android:layout_marginStart="85dp"
        android:layout_weight="1"
        android:text="FAQs"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="MissingConstraints,RtlCompat"
        tools:layout_editor_absoluteX="260dp"
        tools:layout_editor_absoluteY="273dp" />

    <ImageButton
        android:id="@+id/aboutus"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="58dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/ic_location_city_black_24dp"
        tools:ignore="MissingConstraints,RtlCompat,VectorDrawableCompat"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="205dp" />

    <ImageButton
        android:id="@+id/faqs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView3"
        android:layout_alignStart="@+id/aboutus"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/ic_live_help_black_24dp"
        tools:ignore="MissingConstraints,RtlCompat,VectorDrawableCompat"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="205dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="173dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/logo"
        tools:ignore="MissingConstraints,RtlCompat"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="0dp" />

</RelativeLayout>